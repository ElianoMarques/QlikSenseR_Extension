.qv-object-R div.qv-object-content-container {
	overflow: auto;
}
.qv-object-R td, th {
	padding: 3px 10px;
	border-top: 1px solid #fff;
	border-bottom: 1px solid #ddd;
	border-right: 1px solid #ddd;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	vertical-align: middle;
	cursor: default;
	font-size: 12px;
}
.qv-object-R td.numeric {
	text-align: right;
}


.qv-object-R  table { border-collapse: collapse; text-align: left; width: 100%; } 
.qv-object-R  {font: normal 12px/150% Arial, Helvetica, sans-serif; background: #fff; overflow: hidden; border: 1px solid #006699; -webkit-border-radius: 3px; -moz-border-radius: 3px; border-radius: 3px; }
.qv-object-R  table td, .datagrid table th { padding: 3px 10px; }.datagrid table thead th {
background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #00090D), color-stop(1, #000508) );background:-moz-linear-gradient( center top, #00090D 5%, #000508 100% );filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#00090D', endColorstr='#000508');background-color:#00090D; color:#FFFFFF; font-size: 15px; font-weight: bold; } 
.qv-object-R  table thead th:first-child { border: none; }.datagrid table tbody td { color: #000203; font-size: 12px;border-bottom: 1px solid #F4E402;font-weight: normal; }
.qv-object-R  table tbody td:first-child { border-left: none; }
.qv-object-R  table tbody tr:last-child td { border-bottom: none; }



.qv-object-R #ROutputsPlot {
    height: 500px;
}

.qv-object-R #ROutputsPlot2 {
    height: 600px;
    border: solid gray 1px;
	 border-style: hidden;
}

.qv-object-R button {
	width: 30%;
	-moz-box-shadow:inset 0px 1px 0px 0px #cae3fc;
	-webkit-box-shadow:inset 0px 1px 0px 0px #cae3fc;
	box-shadow:inset 0px 1px 0px 0px #cae3fc;
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #79bbff), color-stop(1, #4197ee) );
	background:-moz-linear-gradient( center top, #79bbff 5%, #4197ee 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#79bbff', endColorstr='#4197ee');
	background-color:#79bbff;
	-webkit-border-top-left-radius:20px;
	-moz-border-radius-topleft:20px;
	border-top-left-radius:20px;
	-webkit-border-top-right-radius:20px;
	-moz-border-radius-topright:20px;
	border-top-right-radius:20px;
	-webkit-border-bottom-right-radius:20px;
	-moz-border-radius-bottomright:20px;
	border-bottom-right-radius:20px;
	-webkit-border-bottom-left-radius:20px;
	-moz-border-radius-bottomleft:20px;
	border-bottom-left-radius:20px;
	text-indent:0;
	border:1px solid #469df5;
	display:inline-block;
	color:#ffffff;
	font-family:Arial;
	font-size:15px;
	font-weight:bold;
	font-style:normal;
	height:40px;
	line-height:40px;
	width:100px;
	text-decoration:none;
	text-align:center;
	text-shadow:1px 1px 0px #287ace;
}
